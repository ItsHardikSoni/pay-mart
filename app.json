{
  "expo": {
    "name": "Pay Mart",
    "slug": "pay-mart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-3.png",
    "scheme": "paymart",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to scan product barcodes.",
        "NSMicrophoneUsageDescription": "This app does not require microphone access, but this permission is requested by the camera module.",
        "NSLocationWhenInUseUsageDescription": "This app needs your location to verify that you are in the correct store for barcode scanning.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to verify that you are in the correct store for barcode scanning, even when the app is in the background.",
        "UIBackgroundModes": [
          "location",
          "location"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.hardik.paymart"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/app-icon-3.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "jsEngine": "hermes",
      "package": "com.hardik.paymart"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app-icon-3.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon-3.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "40e85fec-372e-4f04-9186-5f3e79e9c3c1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5e4bc317-a620-44f0-a1c3-8f501b3c8de4"
    },
    "owner": "astitvasoni"
  }
}
